# Supabase Configuration
# Your Supabase project URL from the project settings
VITE_SUPABASE_URL=your_supabase_project_url

# Your Supabase anon/public key from the project API settings
# Used for anonymous user access and public data
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Revolut Payment Configuration
# Your Revolut sandbox API URL
VITE_REVOLUT_API_URL=https://sandbox-merchant.revolut.com/api/1.0
# Your Revolut sandbox API key
VITE_REVOLUT_SANDBOX_API_KEY=your_revolut_sandbox_api_key
# Your Revolut sandbox public key for the checkout widget
VITE_REVOLUT_SANDBOX_PK=your_revolut_sandbox_public_key

# API Server Configuration
PORT=3000

# Optional: Analytics Configuration
# Enable/disable analytics tracking (true/false)
VITE_ENABLE_ANALYTICS=true

# Optional: Map Configuration
# Default map center coordinates
VITE_DEFAULT_MAP_LAT=51.505
VITE_DEFAULT_MAP_LNG=-0.09
VITE_DEFAULT_MAP_ZOOM=13

# Optional: Feature Flags
# Enable/disable experimental features (true/false)
VITE_ENABLE_EXPERIMENTAL=false
